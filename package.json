{
  "name": "react-scaffold",
  "version": "1.0.0",
  "description": "Leverages the latest webpack has to offer in order to quickly get started writing a Universal React application.",
  "main": "index.js",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server:dev": "BABEL_ENV=server NODE_ENV=development webpack --config ./build/webpack/webpack.server.config.js",
    "dev": "npm run build:server:dev && NODE_ENV=development node ./dist/server.js",
    "serve": "NODE_ENV=development babel-node ./src/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wikiwong/react-scaffold.git"
  },
  "keywords": [
    "react",
    "scaffold",
    "es6",
    "webpack"
  ],
  "author": "Rob Wong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wikiwong/react-scaffold/issues"
  },
  "homepage": "https://github.com/wikiwong/react-scaffold#readme"
}
